GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/Choosy" "${PODS_ROOT}/Headers/Mantle" "${PODS_ROOT}/Headers/Reachability"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers" -isystem "${PODS_ROOT}/Headers/Choosy" -isystem "${PODS_ROOT}/Headers/Mantle" -isystem "${PODS_ROOT}/Headers/Reachability"
OTHER_LDFLAGS = -ObjC -framework Foundation -framework SystemConfiguration
PODS_ROOT = ${SRCROOT}/../Pods